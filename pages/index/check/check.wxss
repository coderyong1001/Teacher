.check-container {
  background-color: #f5f5f5;
  padding-top: 24rpx;
  height: 100%;
}
.check-container .check-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 36rpx 0 60rpx 0;
  color: #333;
  background: #ffffff;
  box-shadow: 0px -1px 0px 0px #eeeeee;
}
.check-container .check-wrapper .submit-success {
  font-size: 40rpx;
  line-height: 56rpx;
  margin: 32rpx 0 16rpx 0;
}
.check-container .check-wrapper .submit-info {
  font-size: 28rpx;
  color: #999;
  line-height: 40rpx;
  margin-bottom: 48rpx;
}
.check-container .check-wrapper .gointo-order {
  display: flex;
  align-items: center;
}
.check-container .check-wrapper .gointo-order .cutdown {
  color: #999;
  font-size: 28rpx;
  line-height: 40rpx;
}
.check-container .check-wrapper .gointo-order .my-order-btn {
  width: 144rpx;
  height: 56rpx;
  border-radius: 4rpx;
  border: 1rpx solid #333333;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
}
