.cell-container .cell-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 54rpx;
}
.cell-container .cell-item .item-icon-box {
  display: flex;
  align-items: center;
  margin-right: 20rpx;
}
.cell-container .cell-item .item-icon-box .item-icon {
  width: 50rpx;
  height: 50rpx;
}
.cell-container .cell-item .item-name-value {
  width: 100%;
  font-size: 34rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 50rpx 48rpx 50rpx 0;
}
.cell-container .cell-item .item-name-value .name-value {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}
.cell-container .cell-item .item-name-value .name-value .item-value {
  color: #999;
  max-width: 470rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.cell-container .cell-item .item-name-value .name-value .item-value .head-value {
  width: 76rpx;
  height: 76rpx;
  border-radius: 4rpx;
}
.cell-container .cell-item .item-name-value .arrow {
  margin-left: 48rpx;
}
.cell-container .cell-item .item-name-value.pl-8 {
  padding-left: 16rpx;
}
.cell-container .space {
  height: 48rpx;
  background: #f5f5f5;
}
.name-value-name {
  flex: 0 0 auto;
  padding-right: 30rpx;
}
.marginleft {
  margin-left: 5rpx;
}