.order-space {
  height: 20rpx;
  background-color: #f5f5f5;
}
.order-info-container {
  padding: 48rpx 48rpx 0 48rpx;
  box-sizing: border-box;
}
.order-info-container .order-info-box {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 128rpx;
  padding-bottom: 48rpx;
  font-size: 28rpx;
}
.order-info-container .order-info-box .order-item-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.order-info-container .order-info-box .order-item-box .order-num {
  font-size: 32rpx;
  font-weight: 500;
  color: #000;
  line-height: 32rpx;
}
.order-info-container .order-info-box .order-item-box .activity-name {
  color: #333;
  line-height: 28rpx;
  max-width: 470rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.order-info-container .order-info-box .order-item-box .activity-time {
  color: #999;
  line-height: 28rpx;
}
.order-info-container .order-info-box .order-item-box .pay-status {
  color: #999;
  line-height: 28rpx;
}
.order-info-container .order-info-box .order-item-box .pay-status.success {
  color: #13AB49;
}
.order-info-container .order-info-box .order-item-box .activity-type {
  color: #999;
  line-height: 28rpx;
}
.order-info-container .order-info-box .order-item-box .pay-money {
  font-size: 36rpx;
  font-weight: 500;
  color: #F24724;
  line-height: 50rpx;
}
.order-info-container .list-btn {
  height: 108rpx;
  font-size: 30rpx;
  border-top: 1rpx solid #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.order-info-container .list-btn .list-common-btn {
  background-color: #018389;
  color: #fff;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 20rpx;
  box-sizing: border-box;
  padding: 10rpx 28rpx;
}
.order-info-container .list-btn .list-common-btn.cancel-btn {
  border: 2rpx solid #018389;
  background-color: #fff;
  color: #018389;
}
.black-back {
  height: 100vh;
  width: 100vw;
  background-color: rgba(0, 0, 0, 0.8);
  z-index: 5;
  position: fixed;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.applicants-list {
  width: 560rpx;
  max-height: 750rpx;
  background-color: white;
  border-radius: 8rpx;
  position: relative;
}
.applicants-box-title {
  padding: 50rpx 0;
  text-align: center;
  font-size: 34rpx;
  color: #333333;
}
.applicants-box-comment {
  padding-left: 70rpx;
  font-size: 30rpx;
  line-height: 44rpx;
  font-weight: 400;
  color: #666666;
}
.block-box {
  max-height: 300rpx;
}
.applicants-item {
  padding: 0 70rpx;
  display: flex;
  align-items: center;
  margin-top: 32rpx;
}
.applicants-item-checkbox {
  width: 36rpx;
  height: 36rpx;
  border: 2rpx solid #DBDBDB;
  box-sizing: border-box;
  margin-right: 24rpx;
}
.applicants-item-checked {
  width: 36rpx;
  height: 36rpx;
  margin-right: 24rpx;
}
.applicants-item-name {
  font: 300 26rpx/36rpx;
  font-size: 26rpx;
  font-weight: 300;
  line-height: 36rpx;
  color: #333333;
}
.applicants-refund-money {
  margin: 32rpx 0;
  padding: 0 70rpx;
  font-size: 26rpx;
  line-height: 36rpx;
  font-weight: 400;
  color: #333333;
}
.orange {
  color: #F24724;
}
.applicants-opration {
  display: flex;
  text-align: center;
  font-size: 34rpx;
  font-weight: 400;
  line-height: 34rpx;
  border-top: 1rpx solid #EEEEEE;
}
.applicants-cancel{
  flex: 1 1 auto;
  color: #333333;
  padding: 32rpx 0;
  border-right: 1rpx solid #EEEEEE;
}
.applicants-confirm{
  flex: 1 1 auto;
  color: #018389;
  padding: 32rpx 0;
}
.applicants-close{
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  right: 24rpx;
  top: 24rpx;
}